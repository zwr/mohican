%h2 Log in
= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f|
  .field
    = f.label :email
    %br
    = f.email_field :email, autofocus: true # , value: 'mohican@zwr.fi'
  .field
    = f.label :password
    %br
    = f.password_field :password # , value: 'mohican123'
  - if devise_mapping.rememberable?
    .field
      = f.check_box :remember_me, checked: 'checked'
      = f.label :remember_me
  .actions
    = f.submit "Log in"
= render "devise/shared/links"
